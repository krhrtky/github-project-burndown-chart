{
  "name": "clientv2",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npm run openapi:gen && webpack --mode production",
    "dev": "webpack serve --mode development --port 3000",
    "gql:code:gen": "graphql-codegen --config codegen.yml",
    "gql:code:gen:watch": "graphql-codegen --config codegen.yml -w",
    "openapi:gen": "openapi-generator-cli generate -i $OPENAPI_SCHEMA_PATH -g typescript-fetch -o src/generated/openapi --additional-properties=typescriptThreePlus=true && ls -d src/generated/openapi/models/* | xargs -L1 gsed -i '1s|^|// @ts-nocheck\\n|'",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@apollo/client": "^3.3.19",
    "@geist-ui/react": "^2.1.5",
    "@geist-ui/react-icons": "^1.0.1",
    "@reduxjs/toolkit": "^1.5.1",
    "@types/react-infinite-scroller": "^1.2.1",
    "chart.js": "^3.3.2",
    "connected-react-router": "^6.9.1",
    "firebase": "^9.0.0-beta.1",
    "formik": "^2.2.8",
    "graphql": "^15.5.0",
    "react": "^17.0.2",
    "react-chartjs-2": "^3.0.3",
    "react-dom": "^17.0.2",
    "react-infinite-scroller": "^1.2.4",
    "react-markdown": "^6.0.2",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.0",
    "redux-persist": "^6.0.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.21.4",
    "@graphql-codegen/typescript": "1.22.0",
    "@graphql-codegen/typescript-operations": "1.17.16",
    "@graphql-codegen/typescript-react-apollo": "2.2.4",
    "@openapitools/openapi-generator-cli": "^2.3.5",
    "@types/node": "^15.6.0",
    "@types/react": "^17.0.6",
    "@types/react-dom": "^17.0.5",
    "@types/react-redux": "^7.1.16",
    "@types/react-router-dom": "^5.1.7",
    "@types/webpack-dev-server": "^3.11.4",
    "@typescript-eslint/eslint-plugin": "^4.28.3",
